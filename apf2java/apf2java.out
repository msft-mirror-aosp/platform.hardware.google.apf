        gen.defineLabel("LABEL_104");
        gen.addLoadImmediate(R0, 38);
        gen.addLoadImmediate(R1, -80);
        gen.addJumpIfBytesAtR0NotEqual(hexStringToByteArray("c0a801ec"), "LABEL_582");
